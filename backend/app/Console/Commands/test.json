        "abilities": [
            {
                "characteristics": [
                    {
                        "$text": "This model has a transport capacity of 12 Adeptus Astartes\u00a0Infantry models. Each Jump Pack, Wulfen, Gravis or\u00a0Terminator model takes up the space of 2 models and each\u00a0Centurion model takes up the space of 3 models.",
                        "name": "Description",
                        "typeId": "9b8f-694b-e5e-b573"
                    }
                ],
                "id": "6943-fbd0-f8ec-a693",
                "name": "Transport",
                "hidden": false,
                "typeId": "9cc3-6d83-4dd3-9b64",
                "typeName": "Abilities",
                "from": "entry"
            },
            {
                "characteristics": [
                    {
                        "$text": "If a unit disembarks from this Transport before\u00a0it moves, until the end of the turn, that unit is eligible to\u00a0charge in a turn in which it Advanced.",
                        "name": "Description",
                        "typeId": "9b8f-694b-e5e-b573"
                    }
                ],
                "id": "58cc-7fec-e711-e00d",
                "name": "Into the Foe",
                "hidden": false,
                "typeId": "9cc3-6d83-4dd3-9b64",
                "typeName": "Abilities",
                "from": "entry"
            },
            {
                "characteristics": [
                    {
                        "$text": "While this model has 1-6 wounds remaining, each time this\u00a0model makes an attack, subtract 1 from the Hit roll.",
